<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:v="clr-namespace:System;assembly=mscorlib">

    <v:String x:Key="loginLabelText">Log In</v:String>
    <v:String x:Key="signInLabelText">Sign Up</v:String>
    <v:String x:Key="firstTimeText">First time here? Sign Up</v:String>
    <v:String x:Key="secondTimeText">Already have an account?</v:String>

    <v:String x:Key="remembermeLabelText">Remember me</v:String>

    <!-- Login page errors -->
    <v:String x:Key="errorEmptyValues">Username and password can't be empty</v:String>
    <v:String x:Key="errorWrongPassword">Wrong password</v:String>
    <v:String x:Key="errorExistAccount">Account is not existing</v:String>
    <!---->

    <!-- Sign up page messages -->
    <v:String x:Key="errorLoginMuch">Login should be less than 30 symbols</v:String>
