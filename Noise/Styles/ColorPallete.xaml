<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <!-- Dark theme color palette -->
    <SolidColorBrush x:Key="PrimaryAppColor" Color="#141414"/>

    <SolidColorBrush x:Key="PrimaryTextColor" Color="#FFFFFF"/>
    <SolidColorBrush x:Key="SecondaryTextColor" Color="#909090"/>

    <SolidColorBrush x:Key="SecondaryAppColor" Color="#252525"/>
    <SolidColorBrush x:Key="ThirdAppColor" Color="#38b000"/>

    <SolidColorBrush x:Key="MainTextColor" Color="#404040"/>

    <!-- Data Grid Color Pallete -->
    <SolidColorBrush x:Key="HoverDataGridItem" Color="#303030"/>
    <SolidColorBrush x:Key="SelectedDataGridItem" Color="#404040"/>


    <!-- END OF DARK THEME COLOR PALETE -->
</ResourceDictionary>